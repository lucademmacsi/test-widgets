{
  "name": "widgets",
  "version": "0.0.0",
  "scripts": {
    "build:yucca-widgets": "ng build --prod --project yucca-widgets",
    "publish:yucca-widgets": "npm publish --verbose ./projects/yucca-widgets/dist",
    "publish:yucca-widgets --dry-run": "npm publish --dry-run --verbose ./projects/yucca-widgets/dist",
    "build:yucca-elements": "ng build --prod --project yucca-elements --output-hashing none --output-path ./projects/yucca-elements/dist && npm run pack:yucca-elements",
    "pack:yucca-elements": "concat ./projects/yucca-elements/dist/polyfills-es5.js ./projects/yucca-elements/dist/runtime.js ./projects/yucca-elements/dist/main.js > projects/yucca-elements/dist/yucca-elements.js ",
    "build:yucca-elements-dev": "ng build --project yucca-elements && npm run pack:yucca-elements-dev",
    "pack:yucca-elements-dev": "concat ./projects/yucca-elements/dist/polyfills-es5.js ./projects/yucca-elements/dist/runtime.js ./projects/yucca-elements/dist/main.js ./projects/yucca-elements/dist/vendor.js ./projects/yucca-elements/dist/styles.js> projects/yucca-elements/dist/yucca-elements.js ",
    "publish:yucca-elements": "npm publish --verbose ./projects/yucca-elements/",
    "publish:yucca-elements --dry-run": "npm publish --dry-run --verbose ./projects/yucca-elements/",
    "build:prod:yucca-reference": "ng build --prod --aot=false --base-href /dashboard/ --project yucca-reference --output-path ./projects/yucca-reference",
    "start:yucca-reference": "ng serve --port 4204 --project yucca-reference",
    "start:yucca-widgets-test": "ng serve --project yucca-widgets-test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.12",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/elements": "^9.1.12",
    "@angular/forms": "~9.1.9",
    "@angular/localize": "~9.1.9",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@fortawesome/angular-fontawesome": "^0.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@ng-bootstrap/ng-bootstrap": "^6.2.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^5.0.0",
    "@types/c3": "^0.7.4",
    "@types/d3-sankey": "^0.11.0",
    "bootstrap": "^4.4.0",
    "c3": "^0.7.20",
    "d3-sankey": "^0.12.3",
    "document-register-element": "^1.8.1",
    "elements-zone-strategy": "^10.0.0",
    "ngx-toastr": "^12.1.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular-devkit/build-ng-packagr": "~0.901.12",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "concat": "^1.0.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "npm-cli-login": "^0.1.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}